  - introTextData: "\u3044\u3064\u306E\u9593\u306B\u304B\u6D88\u3048\u3066\u3044\u305F\u98A8\u666F\u3002 "
  - introTextData: "\u6C17\u3065\u304B\u306A\u3044\u3046\u3061\u306B\u3001\u8996\u754C\u304B\u3089\u629C\u3051\u843D\u3061\u3066\u3044\u305F\u3002  "
